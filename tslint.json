{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "max-classes-per-file": 0,
    "one-line": false,
    "object-literal-sort-keys": false,
    "no-string-literal": false,
    "object-literal-key-quotes": false,
    "no-console": false,
    "no-empty": false,
    "variable-name": ["allow-leading-underscore"]
  },
  "rulesDirectory": []
}
